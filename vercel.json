{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/SRC/(.*)",
      "dest": "/SRC/$1"
    },
    {
      "src": "/Data/(.*)",
      "dest": "/Data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
